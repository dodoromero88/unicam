    <title>Hello, <%= student.name%> </title>
  </head>
  <body>
    <div class="row">
      <div class="col-sm">
          <img src="https://www.unimc.it/@@site-logo/unimc-logo.png" />
      </div>
      <div class="col-sm">
      </div>
      <div class="col-sm">
        <nav aria-label="breadcrumb">
